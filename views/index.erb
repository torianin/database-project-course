    <link href="/css/jquery.terminal.css" rel="stylesheet"/>
    <script>
    jQuery(document).ready(function($) {
        $('body').terminal(function(command, term) {
            $.post("ask",
            {
                query:command,
            },
            function(data,status){
                term.echo(data);
            });
        }, {
        greetings: '[[b;green;black]Witaj w coffeeshop\r\n>]',
        name: 'login',
        height: 200,
        width: 450,
        prompt: ''});
    });
    </script>
